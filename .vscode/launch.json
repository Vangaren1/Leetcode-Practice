{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "python",
        "name": "Launch Python Solution",
        "request": "launch",
        "program": "${file}",
        "console": "integratedTerminal"
      },
      {
        "type": "java",
        "name": "Debug Current Solution.java",
        "request": "launch",
        "mainClass": "${fileBasenameNoExtension}",
        "cwd": "${fileDirname}",
        "vmArgs": "-ea"
      }
    ]
  }
  